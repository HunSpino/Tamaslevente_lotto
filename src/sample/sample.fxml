<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" />
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" />
   </rowConstraints>
   <columnConstraints>
       <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
       <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
   </columnConstraints>
   <children>
      <Button fx:id="btnSorsolRendez" alignment="CENTER" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#sorsolRendezClick" prefWidth="100.0" text="Sorsol" />
      <Label fx:id="sorszam" alignment="CENTER_RIGHT" text="0" GridPane.columnIndex="1" />
      <ListView fx:id="sorszamLista" prefHeight="30.0" prefWidth="200.0" GridPane.rowIndex="1" />
      
   </children>
</GridPane>
